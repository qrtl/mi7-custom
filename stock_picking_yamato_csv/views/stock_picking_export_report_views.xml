<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_stock_picking_export_report_tree" model="ir.ui.view">
        <field name="name">stock.picking.export.report.tree</field>
        <field name="model">stock.picking.export.report</field>
        <field name="arch" type="xml">
            <tree string="Stock Picking Export Report" editable="top">
                <field name="move_id" />
                <field name="picking_id" />
                <field name="is_exported" />
                <field name="request_categ" />
                <field name="shipper_code" />
                <field name="ship_categ" />
                <field name="carrier_code" />
                <field name="whs_code" />
                <field name="dlv_rqstd_time_categ" />
                <field name="dlv_service_categ" />
                <field name="amount" />
                <field name="b2_consump_amt" />
            </tree>
        </field>
    </record>
    <record id="action_stock_picking_export_report" model="ir.actions.act_window">
        <field name="name">Stock Picking Export Report</field>
        <field name="res_model">stock.picking.export.report</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
    </record>
    <menuitem
        action="action_stock_picking_export_report"
        id="stock_picking_export_report_menu"
        parent="stock.menu_warehouse_report"
        sequence="170"
    />
</odoo>
