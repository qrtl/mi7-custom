<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- <record id="view_data_import" model="ir.ui.view">
        <field name="name">view.data.import</field>
        <field name="model">data.import</field>
        <field name="arch" type="xml"> -->

    <record id="view_stock_picking_import" model="ir.ui.view">
        <field name="name">view.stock.picking.import</field>
        <field name="model">stock.picking.import</field>
        <field name="inherit_id" ref="base_data_import.view_data_import"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='cancel']" position="before">
                <button
                    string="Import Data"
                    name="import_stock_picking"
                    type="object"
                    class="btn-primary"
                    attrs="{'invisible':[('import_file','=',False)]}"
                />
            </xpath>
        </field>
    </record>
    <record id="action_stock_picking_import_wizard" model="ir.actions.act_window">
        <field name="name">Picking CSV File</field>
        <field name="res_model">stock.picking.import</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_stock_picking_import" />
        <field name="target">new</field>
        <field name="context">{}</field>
    </record>
    <menuitem
        id="stock_picking_import_wizard_menu"
        name="Picking Import"
        parent="stock.menu_stock_warehouse_mgmt"
        action="action_stock_picking_import_wizard"
        sequence="10"
    />
    <menuitem
        id="picking_import_log_menu"
        name="Picking Import Log"
        parent="stock.menu_stock_warehouse_mgmt"
        action="base_data_import.data_import_log_action"
        sequence="20"
    />
</odoo>
